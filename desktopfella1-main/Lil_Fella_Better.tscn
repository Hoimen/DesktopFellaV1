[sub_resource type="GDScript" id="GDScript_t6gb6"]
script/source = "extends VBoxContainer   # CODE IS BROKEN sends openrouter msg twice

@export var conversation: RichTextLabel
@export var prompt: LineEdit
@export var chat: ChatRequester

# Defalt mode
enum Mode { LOCAL, OPENROUTER }
@export var default_mode: Mode = Mode.LOCAL

# Local or just Ollama 
@export var local_host: String = \"http://127.0.0.1\" # Your ip, ha ha i hacked you
@export var local_port: int = 11434
@export var local_model: String = \"llama3.2\"
@export var local_temperature: float = 0.2

# Open router
@export var or_api_key: String = \"\"
@export var or_model: String = \"openai/gpt-4o-mini\"
@export var or_temperature: float = 0.2
@export var or_max_tokens: int = 256                # keeps cost tiny because im poor
@export var or_http_referer: String = \"\"            # optional
@export var or_title: String = \"\"                   # optional

var _mode: Mode
var _buffer: String = \"\"
var _is_warmup: bool = false

func _ready() -> void:
	if conversation == null or prompt == null or chat == null:
		push_error(\"Hook up 'conversation', 'prompt', and 'chat' in the Inspector.\")
		return

	conversation.bbcode_enabled = true
	_mode = default_mode

	_apply_mode()
	prompt.text_submitted.connect(_on_submit)
	chat.new_word.connect(_on_word)
	chat.message_end.connect(_on_end)

func _unhandled_input(event: InputEvent) -> void:
	# Press F1 to toggle Local/OpenRouter
	if event is InputEventKey and event.pressed and not event.echo and event.keycode == KEY_F1:
		_mode = (Mode.OPENROUTER if _mode == Mode.LOCAL else Mode.LOCAL)
		_apply_mode() # must be in text box to activate F1
		var mode_name := (\"Local (Ollama)\" if _mode == Mode.LOCAL else \"OpenRouter\")
		_append_system(\"Switched to %s\" % mode_name)

func _apply_mode() -> void:
	if _mode == Mode.LOCAL:
		chat.set_provider(\"ollama\", true)
		chat.host = local_host
		chat.port = local_port
		chat.model = local_model
		chat.temperature = local_temperature
		chat.api_key = \"\"
		chat.options = {}
	else:
		chat.set_provider(\"openrouter\", true)  # omg my openrouter.gd provider was usefull 
		chat.api_key = or_api_key
		chat.model = or_model
		chat.temperature = or_temperature
		chat.options = {\"max_tokens\": or_max_tokens}
		if or_http_referer != \"\":
			chat.options[\"http_referer\"] = or_http_referer
		if or_title != \"\":
			chat.options[\"x_title\"] = or_title
	_warmup()

func _warmup() -> void:
	_is_warmup = true
	chat.send_message(\"\")            # primes the connection/ i dont think this actually works lol
	await get_tree().create_timer(0.3).timeout
	_is_warmup = false

func _on_submit(text: String) -> void:
	var t := text.strip_edges()
	if t.is_empty():
		return
	_buffer = \"\"
	conversation.append_text(\"\\n[b]You:[/b] %s\\n\" % t)
	chat.send_message(t)
	chat.start_response()
	prompt.clear()

func _on_word(w: String) -> void:
	if _is_warmup:
		return
	_buffer += w

func _on_end(_full: String) -> void:
	var tag := (\"Local\" if _mode == Mode.LOCAL else \"OpenRouter\")
	conversation.append_text(\"[color=#88f]%s:[/color] %s\\n\" % [tag, _buffer]) #88f is the color change later 
	conversation.scroll_to_line(conversation.get_line_count())

func _append_system(msg: String) -> void:
	conversation.append_text(\"\\n[System] %s\\n\" % msg)
"

