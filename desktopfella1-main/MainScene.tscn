[sub_resource type="GDScript" id="GDScript_igvuv"]
script/source = "extends VBoxContainer

# ---------------- UI NODES (hook these up in the Inspector) ----------------
@export var conversation: RichTextLabel
@export var last_ai: RichTextLabel        # NEW: shows only the latest AI reply
@export var prompt: LineEdit
@export var chat: ChatRequester

# Optional UI to edit OpenRouter options at runtime
@export var or_api_key_input: LineEdit
@export var or_tokens_max_input: SpinBox   # single spinner (max only)
@export var or_title_input: LineEdit
@export var or_referer_input: LineEdit

# Buttons to switch providers
@export var btn_openrouter: Button
@export var btn_local: Button

# ---------------- CONFIG ----------------
enum Mode { LOCAL, OPENROUTER }
@export var default_mode: int = Mode.LOCAL

# ---- Local (Ollama) ----
@export var local_host: String = \"http://127.0.0.1\"
@export var local_port: int = 11434
@export var local_model: String = \"llama3.2\"
@export var local_temperature: float = 0.2

# ---- OpenRouter ----
@export var or_api_key: String = \"\"
@export var or_model: String = \"openai/gpt-4o-mini\"
@export var or_temperature: float = 0.2
@export var or_max_tokens: int = 30         # DEFAULT MAX = 30 (SpinBox also starts at 30)
@export var or_http_referer: String = \"\"    # optional
@export var or_title: String = \"\"           # optional

# ---------------- STATE ----------------
var _mode: int
var _buffer: String = \"\"
var _generating: bool = false # prevents double send
var _signals_wired: bool = false

# ---------------- LIFECYCLE ----------------
func _ready() -> void:
	if conversation == null or prompt == null or chat == null:
		push_error(\"Hook up 'conversation', 'prompt', and 'chat' in the Inspector.\")
		return
	conversation.bbcode_enabled = true
	if last_ai:
		last_ai.bbcode_enabled = true
		last_ai.text = \"\"
	_mode = default_mode
	chat.automatically_add_messages = false
	_wire_signals_once()
	_sync_ui_from_vars()
	_apply_mode()

func _wire_signals_once() -> void:
	if _signals_wired: return
	# Text submit
	if not prompt.text_submitted.is_connected(_on_submit):
		prompt.text_submitted.connect(_on_submit)
	# Streaming callbacks
	if not chat.new_word.is_connected(_on_word):
		chat.new_word.connect(_on_word)
	if not chat.message_end.is_connected(_on_end):
		chat.message_end.connect(_on_end)
	# Buttons to switch modes
	if btn_openrouter and not btn_openrouter.pressed.is_connected(_on_click_openrouter):
		btn_openrouter.pressed.connect(_on_click_openrouter)
	if btn_local and not btn_local.pressed.is_connected(_on_click_local):
		btn_local.pressed.connect(_on_click_local)
	# Inputs for OpenRouter options (update vars as user types)
	if or_api_key_input and not or_api_key_input.text_changed.is_connected(_on_api_key_changed):
		or_api_key_input.text_changed.connect(_on_api_key_changed)
	if or_tokens_max_input and not or_tokens_max_input.value_changed.is_connected(_on_tokens_max_changed):
		or_tokens_max_input.value_changed.connect(_on_tokens_max_changed)
	if or_title_input and not or_title_input.text_changed.is_connected(_on_title_changed):
		or_title_input.text_changed.connect(_on_title_changed)
	if or_referer_input and not or_referer_input.text_changed.is_connected(_on_referer_changed):
		or_referer_input.text_changed.connect(_on_referer_changed)
	_signals_wired = true

# Keep F1 toggle too
func _unhandled_input(event: InputEvent) -> void:
	if event is InputEventKey and event.pressed and not event.echo and event.keycode == KEY_F1:
		_mode = Mode.OPENROUTER if _mode == Mode.LOCAL else Mode.LOCAL
		_apply_mode()
		var mode_name: String = \"Local (Ollama)\" if _mode == Mode.LOCAL else \"OpenRouter\"
		_append_system(\"Switched to %s\" % mode_name)

# ---------------- APPLY MODE ----------------
func _apply_mode() -> void:
	# Keep your system prompt as first message; clear the rest
	chat.clear_conversation(true)

	if _mode == Mode.LOCAL:
		chat.set_provider(\"ollama\", true)
		chat.host = local_host
		chat.port = local_port
		chat.model = local_model
		chat.temperature = local_temperature
		chat.api_key = \"\"
		chat.options = {}
	else:
		chat.set_provider(\"openrouter\", true) # requires your openrouter.gd

		# Use current editable values (fall back to exported defaults)
		var api_key: String = or_api_key
		if or_api_key_input:
			api_key = or_api_key_input.text

		var max_tok: int = or_max_tokens
		if or_tokens_max_input:
			max_tok = int(or_tokens_max_input.value)

		var title: String = or_title
		if or_title_input:
			title = or_title_input.text

		var referer: String = or_http_referer
		if or_referer_input:
			referer = or_referer_input.text

		chat.api_key = api_key
		chat.model = or_model
		chat.temperature = or_temperature

		# Only send max_tokens. No minimum anywhere.
		var opts: Dictionary = {\"max_tokens\": max_tok}
		if referer != \"\":
			opts[\"http_referer\"] = referer
		if title != \"\":
			opts[\"x_title\"] = title
		chat.options = opts

	# Update button states (optional visual hint)
	_update_button_states()

# ---------------- SEND / STREAM ----------------
func _on_submit(text: String) -> void:
	var t: String = text.strip_edges()
	if t.is_empty(): return
	if _generating:
		_append_system(\"Still answering‚Ä¶\")
		return
	_generating = true
	prompt.editable = false
	_buffer = \"\"
	conversation.append_text(\"\\n[b]You:[/b] %s\\n\" % t)

	# Clear / prime the \"last reply\" label
	_set_last_ai(\"[i]thinking‚Ä¶[/i]\")

	# Optional: log rough prompt token estimate before sending
	_log_prompt_estimate(t)

	chat.send_message(t)   # append user once
	chat.start_response()  # start exactly once
	prompt.clear()

func _on_word(w: String) -> void:
	# Accumulate only; print once on end
	_buffer += w
	# Live-update the last-ai view
	_set_last_ai(_buffer)

func _on_end(_full: String) -> void:
	# Last-ditch anti-dupe: if buffer is exactly two identical halves, keep one
	if _buffer.length() % 2 == 0:
		var half: String = _buffer.substr(0, _buffer.length() / 2)
		if half + half == _buffer:
			_buffer = half
	var tag: String = \"Local\" if _mode == Mode.LOCAL else \"OpenRouter\"
	conversation.append_text(\"[color=#88f]%s:[/color] %s\\n\" % [tag, _buffer])
	conversation.scroll_to_line(conversation.get_line_count())

	# Finalize the \"last reply\" view
	_set_last_ai(_buffer)

	_generating = false
	prompt.editable = true

	# New: log token usage for the assistant's reply
	_log_token_usage(_buffer)

	# Optional: auto-fallback if OpenRouter has no credits
	if _mode == Mode.OPENROUTER and _buffer.find(\"Insufficient credits\") != -1:
		_append_system(\"OpenRouter credits empty / key limit 0 ‚Äî switching to Local.\")
		_mode = Mode.LOCAL
		_apply_mode()

# ---------------- UI HELPERS ----------------
func _append_system(msg: String) -> void:
	conversation.append_text(\"\\n[System] %s\\n\" % msg)

func _update_button_states() -> void:
	if btn_openrouter:
		btn_openrouter.disabled = (_mode == Mode.OPENROUTER)
	if btn_local:
		btn_local.disabled = (_mode == Mode.LOCAL)

func _sync_ui_from_vars() -> void:
	# Populate inputs with current exported values on startup
	if or_api_key_input:
		or_api_key_input.text = or_api_key

	# Configure single max spinbox and force visible default = 30
	if or_tokens_max_input:
		or_tokens_max_input.min_value = 1
		or_tokens_max_input.max_value = 32768
		or_tokens_max_input.step = 1
		or_tokens_max_input.value = 30   # ensure it shows 30 on startup
		or_max_tokens = int(or_tokens_max_input.value)

	if or_title_input:
		or_title_input.text = or_title
	if or_referer_input:
		or_referer_input.text = or_http_referer
	_update_button_states()

# ---------------- SIGNAL HANDLERS ----------------
func _on_click_openrouter() -> void:
	_mode = Mode.OPENROUTER
	_apply_mode()
	_append_system(\"Switched to OpenRouter\")

func _on_click_local() -> void:
	_mode = Mode.LOCAL
	_apply_mode()
	_append_system(\"Switched to Local (Ollama)\")

func _on_api_key_changed(new_text: String) -> void:
	or_api_key = new_text
	if _mode == Mode.OPENROUTER:
		chat.api_key = new_text

func _on_tokens_max_changed(value: float) -> void:
	or_max_tokens = int(value)
	if _mode == Mode.OPENROUTER:
		var opts: Dictionary = {}
		if typeof(chat.options) == TYPE_DICTIONARY:
			opts = chat.options
		opts[\"max_tokens\"] = or_max_tokens
		chat.options = opts

func _on_title_changed(new_text: String) -> void:
	or_title = new_text
	if _mode == Mode.OPENROUTER:
		var opts: Dictionary = {}
		if typeof(chat.options) == TYPE_DICTIONARY:
			opts = chat.options
		if new_text.strip_edges() == \"\":
			if opts.has(\"x_title\"):
				opts.erase(\"x_title\")
		else:
			opts[\"x_title\"] = new_text
		chat.options = opts

func _on_referer_changed(new_text: String) -> void:
	or_http_referer = new_text
	if _mode == Mode.OPENROUTER:
		var opts: Dictionary = {}
		if typeof(chat.options) == TYPE_DICTIONARY:
			opts = chat.options
		if new_text.strip_edges() == \"\":
			if opts.has(\"http_referer\"):
				opts.erase(\"http_referer\")
		else:
			opts[\"http_referer\"] = new_text
		chat.options = opts

# ---------------- TOKEN LOGGING ----------------
# Tries to retrieve exact token usage from the ChatRequester if available.
# Falls back to a simple heuristic (~4 chars per token).
func _log_token_usage(assistant_text: String) -> void:
	var exact_total: int = -1
	var exact_prompt: int = -1
	var exact_completion: int = -1

	if chat.has_method(\"get_last_usage\"):
		var usage: Dictionary = chat.get_last_usage()
		if typeof(usage) == TYPE_DICTIONARY:
			exact_total = int(usage.get(\"total_tokens\", -1))
			exact_prompt = int(usage.get(\"prompt_tokens\", -1))
			exact_completion = int(usage.get(\"completion_tokens\", -1))
	elif chat.has_method(\"get_last_response_meta\"):
		var meta: Dictionary = chat.get_last_response_meta()
		if typeof(meta) == TYPE_DICTIONARY and meta.has(\"usage\"):
			var u: Dictionary = meta[\"usage\"]
			exact_total = int(u.get(\"total_tokens\", -1))
			exact_prompt = int(u.get(\"prompt_tokens\", -1))
			exact_completion = int(u.get(\"completion_tokens\", -1))

	# Fallback: estimate
	if exact_total < 0:
		var est_completion: int = _estimate_tokens(assistant_text)
		var msg: String = \"Tokens used (estimated): completion ‚âà %d\" % est_completion
		print(msg)
		_append_system(msg)
	else:
		var parts: Array[String] = []
		if exact_total >= 0:
			parts.append(\"total %d\" % exact_total)
		if exact_prompt >= 0:
			parts.append(\"prompt %d\" % exact_prompt)
		if exact_completion >= 0:
			parts.append(\"completion %d\" % exact_completion)
		var msg2: String = \"Tokens used: \" + _join_with_comma(parts)
		print(msg2)
		_append_system(msg2)

func _estimate_tokens(text: String) -> int:
	# Very rough: ~4 chars / token; clamp at least 1.
	return int(ceil(max(1.0, float(text.length()) / 4.0)))

# Simple join helper (typed arrays don't expose .join() at compile time)
func _join_with_comma(arr: Array[String]) -> String:
	var out: String = \"\"
	for i in range(arr.size()):
		if i > 0:
			out += \", \"
		out += arr[i]
	return out

func _log_prompt_estimate(user_text: String) -> void:
	# Lightweight helper to log prompt estimate when sending
	var est: int = _estimate_tokens(user_text)
	print(\"Prompt tokens (estimated): %d\" % est)

# ---------------- helper for the single \"last reply\" view -------------------
func _set_last_ai(text: String) -> void:
	if last_ai == null:
		return
	# Keep it simple: no role tags, just the content
	last_ai.text = text
	last_ai.scroll_to_line(last_ai.get_line_count())
"

